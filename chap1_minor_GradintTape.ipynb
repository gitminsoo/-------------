   "source": [
    "import tensorflow as tf\n",
    "import numpy\n",
    "\n",
    "w=tf.Variable(3.0)\n",
    "x = 2.0\n",
    "\n",
    "with tf.GradientTape() as tape:\n",
    "    y=w*x\n",
    "grad_tf = tape.gradient(y,w).numpy()\n",
    "grad_symbol = x\n",
    "print(f\"D(w*x)=x with {w.numpy()}: Symb->{grad_symbol}, TF->{grad_tf}\")"
   ]
  }
